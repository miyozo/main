package chapter2_2;

public class Sample {

	public static void main(String[] args) {
		int x;
		double data;
		char ch;
		String str;
		x = 20;
		data  = 1.234;
		ch = 'a';
		str = "こんにちは";
		
			System.out.println(x);
			System.out.println(data);
			System.out.println(ch);
			System.out.println(str);
	}

}
